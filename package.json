{
  "name": "architecture",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"vite\" \"typed-scss-modules src/**/*.module.scss --aliasPrefixes.styles src/styles -w --ignoreInitial --nameFormat dashes\"",
    "build": "tsc && vite build",
    "preview": "vite preview --port 3000",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "stylelint": "npx stylelint \"**/*.scss\"",
    "stylelint:fix": "npx stylelint \"**/*.scss\" --fix",
    "prettier:fix": "prettier . --write --cache",
    "plop:cc": "plop create-component",
    "check:styles-types": "typed-scss-modules src/**/*.module.scss --listDifferent --aliasPrefixes.styles src/styles --nameFormat dashes",
    "check:typescript": "tsc",
    "generate-scss-types": "typed-scss-modules src/**/*.module.scss --aliasPrefixes.styles src/styles --nameFormat dashes"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.66.11",
    "@tanstack/react-router": "^1.112.0",
    "@types/node": "^22.13.5",
    "axios": "^1.8.1",
    "date-fns": "^4.1.0",
    "decimal.js": "^10.5.0",
    "humps": "^2.0.1",
    "i18next-browser-languagedetector": "^8.0.4",
    "i18next-http-backend": "^3.0.2",
    "qs": "^6.14.0",
    "react": "18.3.1",
    "react-aria-components": "^1.6.0",
    "react-dom": "18.3.1",
    "react-hot-toast": "^2.5.2",
    "react-i18next": "^15.4.1",
    "react-use": "^17.6.0",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.3.0",
    "@eslint-react/eslint-plugin": "^1.26.2",
    "@tanstack/react-query-devtools": "^5.66.11",
    "@tanstack/router-devtools": "^1.112.0",
    "@types/humps": "^2.0.6",
    "@types/qs": "^6.9.18",
    "@types/react": "18.3.18",
    "@types/react-dom": "18.3.5",
    "@vitejs/plugin-react-swc": "^3.8.0",
    "concurrently": "^9.1.2",
    "eslint": "^9.20.1",
    "eslint-plugin-format": "^1.0.1",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "plop": "^4.0.1",
    "sass": "1.85.1",
    "stylelint": "^16.14.1",
    "stylelint-config-standard-scss": "14.0.0",
    "stylelint-order": "6.0.4",
    "stylelint-prettier": "5.0.3",
    "stylelint-semantic-groups": "1.2.1",
    "typed-scss-modules": "8.1.1",
    "typescript": "~5.7.3",
    "vite": "^6.2.0",
    "vite-plugin-svgr": "^4.3.0"
  }
}
